{
  "name": "matcher",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "stop": "pkill -f 'next' || true",
    "lint": "eslint",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "npx tsx lib/db/scripts/seed.ts",
    "db:drop": "npx tsx lib/db/scripts/drop.ts",
    "db:init": "npx tsx lib/db/scripts/init.ts",
    "db:reset": "npm run db:drop && npm run db:init && npm run db:push && npm run db:seed",
    "codegen": "graphql-codegen --config codegen.ts"
  },
  "dependencies": {
    "@apollo/client": "^4.1.3",
    "@apollo/client-integration-nextjs": "^0.14.4",
    "@apollo/server": "^5.3.0",
    "@as-integrations/next": "^4.1.0",
    "@base-ui/react": "^1.1.0",
    "@radix-ui/react-switch": "^1.2.6",
    "@supabase/supabase-js": "^2.93.3",
    "@tanstack/react-table": "^8.21.3",
    "better-auth": "^1.4.18",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dataloader": "^2.2.3",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.45.1",
    "graphql": "^16.12.0",
    "graphql-tag": "^2.12.6",
    "lucide-react": "^0.563.0",
    "next": "16.1.6",
    "openai": "^6.17.0",
    "postgres": "^3.4.8",
    "prism-react-renderer": "^2.4.1",
    "radix-ui": "^1.4.3",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "shadcn": "^3.8.2",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tw-animate-css": "^1.4.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^6.1.1",
    "@graphql-codegen/typescript": "^5.0.7",
    "@graphql-codegen/typescript-operations": "^5.0.7",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/ui": "^4.0.18",
    "drizzle-kit": "^0.31.8",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^4.0.18"
  }
