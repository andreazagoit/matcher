{
  "name": "matcher",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "stop": "pkill -f 'next' || true",
    "lint": "eslint",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "npx tsx lib/db/seed/index.ts",
    "db:drop": "npx tsx lib/db/scripts/drop.ts",
    "db:init": "npx tsx lib/db/scripts/init.ts",
    "db:reset": "npm run db:drop && npm run db:init && npm run db:push && npm run db:seed",
    "codegen": "graphql-codegen --config codegen.ts",
    "ml:setup": "python3 -m venv python-ml/.venv && python-ml/.venv/bin/pip install -q -r python-ml/requirements.txt && echo 'venv ready'",
    "ml:export": "npx tsx lib/db/scripts/export-ml-data.ts",
    "ml:generate": "cd python-ml && .venv/bin/python -m hgt.synthetic",
    "ml:train": "cd python-ml && .venv/bin/python -m hgt.train --eval-every 5",
    "ml:serve": "cd python-ml && .venv/bin/uvicorn hgt.serve:app --host 0.0.0.0 --port 8000 --reload"
  },
  "dependencies": {
    "@apollo/client": "^4.1.3",
    "@apollo/client-integration-nextjs": "^0.14.4",
    "@apollo/server": "^5.3.0",
    "@as-integrations/next": "^4.1.0",
    "@base-ui/react": "^1.1.0",
    "@better-auth/expo": "^1.4.18",
    "@radix-ui/react-switch": "^1.2.6",
    "@supabase/supabase-js": "^2.93.3",
    "@tanstack/react-table": "^8.21.3",
    "better-auth": "^1.4.18",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "dataloader": "^2.2.3",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.45.1",
    "graphql": "^16.12.0",
    "graphql-tag": "^2.12.6",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.563.0",
    "next": "16.1.6",
    "next-intl": "^4.8.3",
    "nodemailer": "^8.0.1",
    "onnxruntime-node": "^1.24.2",
    "openai": "^6.25.0",
    "postgres": "^3.4.8",
    "prism-react-renderer": "^2.4.1",
    "radix-ui": "^1.4.3",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-leaflet": "^5.0.0",
    "shadcn": "^3.8.2",
    "sonner": "^2.0.7",
    "stripe": "^20.3.1",
    "tailwind-merge": "^3.4.0",
    "tw-animate-css": "^1.4.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^6.1.1",
    "@graphql-codegen/typescript": "^5.0.7",
    "@graphql-codegen/typescript-operations": "^5.0.7",
    "@tailwindcss/postcss": "^4",
    "@types/leaflet": "^1.9.21",
    "@types/node": "^20",
    "@types/nodemailer": "^7.0.10",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/ui": "^4.0.18",
    "drizzle-kit": "^0.31.9",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^4.0.18"
  }
}
